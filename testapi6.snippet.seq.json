{
  "Sequence box": {
    "prefix": "@com.box",
    "body": [
      "$LINE_COMMENT/ BOX rgb(${1}, ${2}, ${0})"
    ],
    "description": "Wrap a box with color"
  },
  "Sequence comment": {
    "prefix": "@com.seq",
    "body": [
      "$LINE_COMMENT/ ${0}"
    ],
    "description": "Do something in context"
  },
  "Sequence request": {
    "prefix": "@req.seq",
    "body": [
      "$LINE_COMMENT/ {${1}} => {${2}}: ${0}"
    ],
    "description": "Request to a service"
  },
  "Sequence response": {
    "prefix": "@res.seq",
    "body": [
      "$LINE_COMMENT/ {${1}} <= {${2}}: ${0}"
    ],
    "description": "Response to another"
  },
  "Sequence throw error": {
    "prefix": "@throw.seq",
    "body": [
      "$LINE_COMMENT/ {${1}} <x {${2}}: Throw { ${0} }"
    ],
    "description": "Throw error then stop"
  },
  "Sequence done": {
    "prefix": "@done.seq",
    "body": [
      "$LINE_COMMENT/ {${1}} <x {${2}}: Response { ${0} }"
    ],
    "description": "Response then stop"
  },
  "Sequence return": {
    "prefix": "@return.seq",
    "body": [
      "$LINE_COMMENT/ Return { ${0} }"
    ],
    "description": "Response then stop"
  },
  "Sequence publish": {
    "prefix": "@pub.seq",
    "body": [
      "$LINE_COMMENT/ {${1}} -> {${2}}: ${0}"
    ],
    "description": "Publish message"
  },
  "Sequence subscribe": {
    "prefix": "@sub.seq",
    "body": [
      "$LINE_COMMENT/ {${1}} <- {${2}}: ${0}"
    ],
    "description": "Consume message"
  },
  "Sequence call next": {
    "prefix": "@call.seq",
    "body": [
      "$LINE_COMMENT/ {${1}} > {${2}}: ${0}"
    ],
    "description": "Call another function"
  },
  "Sequence call back": {
    "prefix": "@back.seq",
    "body": [
      "$LINE_COMMENT/ {${1}} < {${2}}: ${0}"
    ],
    "description": "The function done"
  },
  "Sequence loop": {
    "prefix": "@loop.seq",
    "body": [
      "$LINE_COMMENT/ LOOP ${0}"
    ],
    "description": "LOOP"
  },
  "Sequence parallel": {
    "prefix": "@parallel.seq",
    "body": [
      "$LINE_COMMENT/ PARALLEL ${0}"
    ],
    "description": "For, loop something"
  },
  "Sequence if": {
    "prefix": "@if.seq",
    "body": [
      "$LINE_COMMENT/ IF ${0}"
    ],
    "description": "If condition"
  },
  "Sequence else": {
    "prefix": "@else.seq",
    "body": [
      "$LINE_COMMENT/ ELSE ${0}"
    ],
    "description": "Else condition"
  },
  "Sequence declare func": {
    "prefix": "@func.seq",
    "body": [
      "$LINE_COMMENT/ [${1:Function_Key}]${2:{Context\\}} ${0:description}"
    ],
    "description": "Declare a function"
  },
  "Sequence ref function": {
    "prefix": "@ref.seq",
    "body": [
      "$LINE_COMMENT/ [${1:Function_Key}] ${0:description}"
    ],
    "description": "Reference a function"
  }
}